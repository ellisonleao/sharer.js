{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended"
}
